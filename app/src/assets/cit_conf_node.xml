<?xml version="1.0" encoding="utf-8"?>
<config>
    <properties>
        <!--version-->
        <property 
            name="model" 
            value="ro.product.model">
        </property>
        <property 
            name="internaledition" 
            value="ro.product.internaledition">
        </property>
        <property 
            name="baseband" 
            value="gsm.version.baseband">
        </property>
        <property 
            name="software_title" 
            value="ro.product.version.software">
        </property>
        <property 
            name="software_time" 
            value="ro.build.version.incremental">
        </property>
        <property 
            name="software_buildtime" 
            value="ro.product.date">
        </property>
        <property 
            name="data3_2499" 
            value="gsm.sim.data3">
        </property>
    </properties>

    <methods>
        <!--version-->
        <method 
            name="qcrilhook_factory_test_process" 
            value="FactoryTestProcess">
        </method>
        <method 
            name="qcnvitem_read2497" 
            value="get_NV2497_sn_number">
        </method>
        <!--test_activity-->
        <method 
            name="qcrilhook_factory_write2499" 
            value="writeNvFactoryData">
        </method>
        <!--p_sensor_calibration-->
        <method 
            name="qcnvitems_write_p_sensor_calibration" 
            value="set_p_sensor">
        </method>-->
        <method 
            name="qcnvitems_write_g_sensor_calibration" 
            value="set_g_sensor">
        </method>-->
        <method 
            name="qcnvitems_write_gyro_sensor_calibration" 
            value="set_gyro_sensor">
        </method>-->
    </methods>
    <nodes>
        <!--version-->
        <node 
            name="hw_subtype" 
            value="/sys/devices/soc0/platform_subtype" 
            permission="O_RDONLY" 
            operation="read">
        </node>
        <node 
            name="hw_version" 
            value="/sys/devices/soc0/platform_version" 
            permission="O_RDONLY" 
            operation="read">
        </node>
        <!--button_backlight-->
        <node 
            name="button_backlight" 
            value="/sys/class/leds/button_backlight/brightness" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <!--flashlight-->
        <node 
            name="flashlight" 
            value="/sys/class/leds/torch-light0/brightness" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <!--irled-->
        <node 
            name="irled" 
            value="/sys/class/leds/ir-led-pwm/brightness" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <!--battery_charge_State-->
        <node 
            name="electricity" 
            value="/sys/class/power_supply/battery/current_now" 
            permission="O_RDONLY" 
            operation="read">
        </node>
        <node 
            name="voltage" 
            value="/sys/class/power_supply/battery/voltage_now" 
            permission="O_RDONLY" 
            operation="read">
        </node>
        <node 
            name="temperature" 
            value="/sys/class/power_supply/bms/temp"
            permission="O_RDONLY" 
            operation="read">
        </node>
        <!--flashlight-->
        <node 
            name="led_r" 
            value="/sys/class/leds/red/brightness"
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="led_g" 
            value="/sys/class/leds/green/brightness"
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="led_o" 
            value="/sys/class/leds/blue/brightness"
            permission="O_RDWR" 
            operation="read">
        </node>

        <!--sensors_node-->
        <!--sensors_node_gyro-->
        <node 
            name="gyro_calibration" 
            value="/sys/class/sensors/MPU6050-gyro/self_test" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gyro_x" 
            value="/sys/class/input/input1/offset_x" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gyro_y" 
            value="/sys/class/input/input1/offset_y" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gyro_z" 
            value="/sys/class/input/input1/offset_z" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <!--sensors_node_accel-->
        <node 
            name="gsensor_calibration" 
            value="/sys/class/sensors/MPU6050-accel/self_test" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gsensor_x" 
            value="/sys/class/input/input0/offset_x" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gsensor_y" 
            value="/sys/class/input/input0/offset_y" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="gsensor_z" 
            value="/sys/class/input/input0/offset_z" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <!--sensors_node_psensor-->
        <node 
            name="psensor_calibration" 
            value="/sys/class/input/input3/ps_citvalue" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="psensor_near" 
            value="/data/psensor/psensor_near" 
            permission="O_RDWR" 
            operation="read">
        </node>
        <node 
            name="psensor_far" 
            value="/sys/class/input/input4/ps_5_cal_value" 
            permission="O_RDWR" 
            operation="read">
        </node>

    </nodes>
</config>
